{
  "name": "shopping-api",
  "version": "1.0.0",
  "description": "A RESTful API for an online shopping application built with Node.js, Express, TypeScript, and PostgreSQL.",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "migrate:up": "psql -h $POSTGRES_HOST -U $POSTGRES_USER -d $POSTGRES_DB -f migrations/001_create_tables.up.sql",
    "migrate:down": "psql -h $POSTGRES_HOST -U $POSTGRES_USER -d $POSTGRES_DB -f migrations/001_create_tables.down.sql",
    "test": "tsc && jasmine"
  },
  "author": "Sana Saleh",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/jasmine": "^5.1.12",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^24.8.1",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "jasmine": "^5.12.0",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^7.2.0",
    "typescript": "^5.9.3"
  }
}